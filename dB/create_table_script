-- Tablespace: pg_global

-- DROP TABLESPACE pg_global;

ALTER TABLESPACE pg_global
  OWNER TO postgres;
 CREATE TABLE teams (
    tname           varchar(80),
    teamid          serial
);

CREATE TABLE games (gameid serial PRIMARY KEY,
    gamename VARCHAR (40) UNIQUE NOT NULL
);

CREATE TABLE results (
    roundid          serial,
    rtimestamp       time,
    teamid           int,
    gamenameid       int
);
CREATE TABLE scores (
    roundid       int,
    teamid        int,
    score         int
);
